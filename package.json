{
  "name": "automation1_exercise",
  "version": "1.0.0",
  "description": "Code for my Automation Level 1 Badge project (based on a boilerplate project that uses version 5 of WebdriverIO and TypeScript).",
  "repository": "https://github.com/srossbyers/Automation1_Exercise",
  "scripts": {
    "test": "wdio",
    "test_mobile": "wdio --view=mobile",
    "test_google": "wdio --baseUrl=http://google.com",
    "test_nothing": "wdio --baseUrl=http://nonexistentpage.com",
    "lint": "tslint --fix 'test/**/*.ts' && tslint --fix 'src/**/*.ts'",
    "report": "allure generate ./allure-results --clean && allure open",
    "report-ci": "allure generate allure-results --clean -o allure-report"
  },
  "author": "Steve Ross-Byers",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.17",
    "@wdio/allure-reporter": "^5.18.6",
    "@wdio/cli": "^5.18.7",
    "@wdio/local-runner": "^5.18.7",
    "@wdio/mocha-framework": "^5.18.7",
    "@wdio/selenium-standalone-service": "^5.16.10",
    "@wdio/spec-reporter": "^5.18.7",
    "@wdio/sync": "^5.18.7",
    "allure-commandline": "^2.13.0",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "^3.8.2",
    "webdriverio": "^5.18.7"
  },
  "dependencies": {}
}
